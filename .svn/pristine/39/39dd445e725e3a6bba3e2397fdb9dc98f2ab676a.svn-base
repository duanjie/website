.header-nav {
	position: fixed;
	background-color: #283379 !important;
	border-top: 1px solid #feeff2;
	border-bottom: 1px solid #e4e4e4;
	height: 100px;
	width: 100%;
	z-index: 3000;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	*zoom: 1
}

.header-nav:before,.header-nav:after {
	content: "";
	display: table
}

.header-nav:after {
	clear: both
}

.header-nav .logo {
	display: inline;
	float: left;
	width: 22.5%;
	margin: 0 1.25%;
	position: relative;
	top: 28px;
	padding-left: 15px
}

.header-nav .nav-wrap {
	display: inline;
	float: left;
	width: 47.5%;
	margin: 0 1.25%;
	margin-right: 0
}

.header-nav .nav-wrap .navbar-toggle {
	display: none
}

.header-nav .nav-wrap ul {
	float: right;
	display: inline;
	height: 97px;
	margin: 0
}

.header-nav .nav-wrap ul li {
	display: inline-block
}

.header-nav .nav-wrap ul li a {
	position: relative;
	line-height: 94px;
	display: block;
	text-decoration: none;
	/*导航菜单显示颜色
	color: #eb1d47;*/
	color: white;
	padding: 0 15px;
	font-family: GothamBook, sans-serif;
	font-size: 13px;
	text-transform: uppercase
}
/* 鼠标移动进入导航条 菜单显示的颜色  */
.header-nav .nav-wrap ul li a:hover {
	/*color: #cb153a*/
	color:white;
}

.header-nav .nav-wrap ul li a::after {
	position: absolute;
	top: 101%;
	left: 0;
	width: 100%;
	height: 4px;
	/*导航菜单下浮动的横条
	background: #eb1d47;*/
	background: #99cc00;
	content: '';
	opacity: 0;
	-webkit-transition: opacity .3s, -webkit-transform .3s;
	-moz-transition: opacity .3s, -moz-transform .3s;
	-ms-transition: opacity .3s, -ms-transform .3s;
	-o-transition: opacity .3s, -o-transform .3s;
	transition: opacity .3s, transform .3s;
	-webkit-transform: translateX(100px);
	-moz-transform: translateX(100px);
	-ms-transform: translateX(100px);
	-o-transform: translateX(100px);
	transform: translateX(100px)
}

.header-nav .nav-wrap ul li a:hover::after,.header-nav .nav-wrap ul li a:focus::after
	{
	opacity: 1;
	-webkit-transform: translateX(0px);
	-moz-transform: translateX(0px);
	-ms-transform: translateX(0px);
	-o-transform: translateX(0px);
	transform: translateX(0px)
}

.header-nav .nav-wrap ul .current_page_item a {
	/*鼠标按下后导航菜单显示的颜色  ：即选择页面菜单显示的颜色color: #cb153a*/
	/*color:#01248a;*/
}

.header-nav .nav-wrap ul .current_page_item a::after {
	opacity: 1;
	-webkit-transform: translateX(0px);
	-moz-transform: translateX(0px);
	-ms-transform: translateX(0px);
	-o-transform: translateX(0px);
	transform: translateX(0px)
}

.header-nav .nav-social {
	display: inline;
	float: left;
	width: 22.5%;
	margin: 0 1.25%
}

.header-nav .nav-social .social {
	height: 97px;
